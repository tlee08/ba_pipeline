{
  "user": {
    "format_vid": {
      "height_px": 540,
      "width_px": 960,
      "fps": 25
    },
    "run_dlc": {
      "dlc_config_path": "Z:\\PRJ-BowenLab\\TimLee\\releases\\dlc_models\\aggrModel_960px\\config.yaml"
    },
    "calculate_params": {
      "start_frame": {
        "window_sec": 3,
        "pcutoff": 0.9
      },
      "stop_frame": {
        "dur_sec": 600
      },
      "px_per_mm": {
        "pt_a": "TopLeft",
        "pt_b": "TopRight",
        "dist_mm": 400
      }
    },
    "preprocess": {
      "interpolate_points": {
        "pcutoff": 0.5
      },
      "bodycentre": {
        "bodyparts": [
          "LeftFlankMid",
          "BodyCentre",
          "RightFlankMid",
          "LeftFlankRear",
          "RightFlankRear",
          "TailBase1"
        ]
      },
      "refine_identities": {
        "marked": "mouse1marked",
        "unmarked": "mouse2unmarked",
        "marking": "AnimalColourMark",
        "window_sec": 0.5,
        "metric": "rolling",
        "bodyparts": [
          "LeftFlankMid",
          "BodyCentre",
          "RightFlankMid",
          "LeftFlankRear",
          "RightFlankRear",
          "TailBase1"
        ]
      }
    },
    "evaluate": {
      "keypoints_plot": {
        "bodyparts": ["Nose", "BodyCentre", "TailBase1"]
      },
      "eval_vid": {
        "funcs": ["keypoints", "behavs"],
        "pcutoff": 0.5,
        "colour_level": "individuals",
        "radius": 4,
        "cmap": "rainbow"
      }
    },
    "extract_features": {
      "individuals": ["mouse1marked", "mouse2unmarked"],
      "bodyparts": [
        "LeftEar",
        "RightEar",
        "Nose",
        "BodyCentre",
        "LeftFlankMid",
        "RightFlankMid",
        "TailBase1",
        "TailTip4"
      ]
    },
    "classify_behaviours": {
      "models": [
        "Z:\\PRJ-BowenLab\\TimLee\\resources\\simba_model_aggr\\behav_models\\fight\\model_configs.json",
        "Z:\\PRJ-BowenLab\\TimLee\\resources\\simba_model_aggr\\behav_models\\marked_fight\\model_configs.json",
        "Z:\\PRJ-BowenLab\\TimLee\\resources\\simba_model_aggr\\behav_models\\unmarked_fight\\model_configs.json"
      ],
      "min_window_frames": 2,
      "user_behavs": ["example", "another example"]
    },
    "analyse": {
      "thigmotaxis": {
        "thresh_mm": 50
      },
      "center_crossing": {
        "thresh_mm": 125
      },
      "speed": {
        "smoothing_sec": 1
      },
      "social_distance": {
        "smoothing_sec": 1
      },
      "freezing": {
        "window_sec": 2,
        "thresh_mm": 5,
        "smoothing_sec": 0.2
      },
      "bins_sec": [30, 60, 120, 300],
      "custom_bins_sec": [60, 120, 300, 600]
    }
  }
}
